{
    "name": "lutpiero/open-payroll",
    "description": "Built with Laravel Standalone Package Creator",
    "keywords": ["package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\OpenPayroll\\": "src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\OpenPayroll\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "cleaniquecoders/blueprint-macro": "^5.2",
        "cleaniquecoders/laravel-helper": "^4.2",
        "cleaniquecoders/laravel-observers": "^4.2",
        "cleaniquecoders/money-wrapper": "dev-master",
        "cleaniquecoders/profile": "^5.0",
        "illuminate/support": "~10.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devjaythakkar/money-wrapper.git"
        }
    ],
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\OpenPayroll\\OpenPayrollServiceProvider"
            ],
            "aliases": {
                "OpenPayroll": "CleaniqueCoders\\OpenPayroll\\OpenPayrollFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
